    <!-- http://erlangen-crm.org/current/E18_Physical_Thing -->

    <owl:Class rdf:about="http://erlangen-crm.org/current/E18_Physical_Thing"/>
    


    <!-- http://erlangen-crm.org/current/E1_CRM_Entity -->

    <owl:Class rdf:about="http://erlangen-crm.org/current/E1_CRM_Entity"/>
    


    <!-- http://erlangen-crm.org/current/E21_Person -->

    <owl:Class rdf:about="http://erlangen-crm.org/current/E21_Person">
        <rdfs:comment xml:lang="en">Note for SAWS: this includes any human being, named or otherwise</rdfs:comment>
        <rdfs:label xml:lang="en">person</rdfs:label>
    </owl:Class>
    


    <!-- http://erlangen-crm.org/current/E24_Physical_Man-Made_Thing -->

    <owl:Class rdf:about="http://erlangen-crm.org/current/E24_Physical_Man-Made_Thing"/>
    


    <!-- http://erlangen-crm.org/current/E33_Linguistic_Object -->

    <owl:Class rdf:about="http://erlangen-crm.org/current/E33_Linguistic_Object"/>
    


    <!-- http://erlangen-crm.org/current/E35_Title -->

    <owl:Class rdf:about="http://erlangen-crm.org/current/E35_Title">
        <rdfs:label xml:lang="en">title</rdfs:label>
    </owl:Class>
    


    <!-- http://erlangen-crm.org/current/E36_Visual_Item -->

    <owl:Class rdf:about="http://erlangen-crm.org/current/E36_Visual_Item"/>
    


    <!-- http://erlangen-crm.org/current/E53_Place -->

    <owl:Class rdf:about="http://erlangen-crm.org/current/E53_Place">
        <rdfs:label xml:lang="en">place</rdfs:label>
    </owl:Class>
    


    <!-- http://erlangen-crm.org/current/E56_Language -->

    <owl:Class rdf:about="http://erlangen-crm.org/current/E56_Language">
        <rdfs:comment xml:lang="en">Note for SAWS: This represents language in which a material is written - this is put in the TEI header. 

NB in the TEI header we should use standards for languages, e.g. the more detailed ISO standards at http://www.loc.gov/standards/iso639-2/ , so use “http://id.loc.gov/vocabulary/iso639-2/grc” for Ancient Greek rather than “Greek”</rdfs:comment>
        <rdfs:label xml:lang="en">language</rdfs:label>
    </owl:Class>
    


    <!-- http://erlangen-crm.org/current/E74_Group -->

    <owl:Class rdf:about="http://erlangen-crm.org/current/E74_Group"/>
    


    <!-- http://erlangen-crm.org/efrbroo/F10_Person -->

    <owl:Class rdf:about="http://erlangen-crm.org/efrbroo/F10_Person"/>
    


    <!-- http://erlangen-crm.org/efrbroo/F15_Complex_Work -->

    <owl:Class rdf:about="http://erlangen-crm.org/efrbroo/F15_Complex_Work"/>
    


    <!-- http://erlangen-crm.org/efrbroo/F22_Self-Contained_Expression -->

    <owl:Class rdf:about="http://erlangen-crm.org/efrbroo/F22_Self-Contained_Expression"/>
    


    <!-- http://erlangen-crm.org/efrbroo/F23_Expression_Fragment -->

    <owl:Class rdf:about="http://erlangen-crm.org/efrbroo/F23_Expression_Fragment"/>
    


    <!-- http://erlangen-crm.org/efrbroo/F2_Expression -->

    <owl:Class rdf:about="http://erlangen-crm.org/efrbroo/F2_Expression"/>
    


    <!-- http://erlangen-crm.org/efrbroo/F4_Manifestation_Singleton -->

    <owl:Class rdf:about="http://erlangen-crm.org/efrbroo/F4_Manifestation_Singleton"/>
    


    <!-- http://purl.org/saws/ontology#AbstractManuscriptText -->

    <owl:Class rdf:about="http://purl.org/saws/ontology#AbstractManuscriptText">
        <rdfs:subClassOf rdf:resource="http://purl.org/saws/ontology#OriginalMaterial"/>
        <rdfs:comment xml:lang="en">The text that is written in an manuscript</rdfs:comment>
        <rdfs:label xml:lang="en">abstract manuscript text</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/saws/ontology#AttributedAuthor -->

    <owl:Class rdf:about="http://purl.org/saws/ontology#AttributedAuthor">
        <rdfs:subClassOf rdf:resource="http://erlangen-crm.org/current/E21_Person"/>
        <owl:disjointWith rdf:resource="http://purl.org/saws/ontology#Editor"/>
        <rdfs:comment xml:lang="en">Person to whom original authorship of a linguistic object is attributed, as the source of that linguistic object</rdfs:comment>
        <rdfs:label xml:lang="en">attributed author</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/saws/ontology#CollectionInstance -->

    <owl:Class rdf:about="http://purl.org/saws/ontology#CollectionInstance">
        <rdfs:subClassOf rdf:resource="http://purl.org/saws/ontology#OriginalMaterial"/>
        <rdfs:comment xml:lang="en">The gnomologium or other compilation/collection on which we are working</rdfs:comment>
        <rdfs:label xml:lang="en">collection instance</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/saws/ontology#Comment -->

    <owl:Class rdf:about="http://purl.org/saws/ontology#Comment">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://erlangen-crm.org/current/E33_Linguistic_Object"/>
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F23_Expression_Fragment"/>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://purl.org/saws/ontology#Marginalia"/>
        <rdfs:comment xml:lang="en">Comment on a part of the material, usually from the modern editor. 

N.B. The Comment class is for comments on a material that do not physically exist on that material. If a comment has been physically written on a material (usually in the margin), then this should be represented as an instance of Marginalia. </rdfs:comment>
        <rdfs:label xml:lang="en">comment</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/saws/ontology#ContentItem -->

    <owl:Class rdf:about="http://purl.org/saws/ontology#ContentItem">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://erlangen-crm.org/current/E33_Linguistic_Object"/>
                    <rdf:Description rdf:about="http://erlangen-crm.org/efrbroo/F23_Expression_Fragment"/>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A basic unit of interest within the Material: a division of a collection instance/section, for more exact reference than when using a section – this identifies basic units of interest in the material for SAWS (marked in TEI as &lt;seg&gt;)</rdfs:comment>
        <rdfs:label xml:lang="en">content item</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/saws/ontology#DecorativeItem -->

    <owl:Class rdf:about="http://purl.org/saws/ontology#DecorativeItem">
        <rdfs:subClassOf rdf:resource="http://erlangen-crm.org/current/E36_Visual_Item"/>
        <rdfs:comment xml:lang="en">Decorative element (picture, ornamentation, etc) within the material, either meaning-bearing or non meaning-bearing</rdfs:comment>
        <rdfs:label xml:lang="en">decorative item</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/saws/ontology#Definition -->

    <owl:Class rdf:about="http://purl.org/saws/ontology#Definition">
        <rdfs:subClassOf rdf:resource="http://purl.org/saws/ontology#ContentItem"/>
        <rdfs:comment xml:lang="en">A content item that is defining a concept or term used in the material</rdfs:comment>
        <rdfs:label xml:lang="en">definition</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/saws/ontology#Edition -->

    <owl:Class rdf:about="http://purl.org/saws/ontology#Edition">
        <rdfs:subClassOf rdf:resource="http://purl.org/saws/ontology#Material"/>
        <owl:disjointWith rdf:resource="http://purl.org/saws/ontology#OriginalMaterial"/>
        <rdfs:comment xml:lang="en">Any edited material, known or unknown, including texts such as the Bible and Kekaumenos</rdfs:comment>
        <rdfs:label xml:lang="en">edition</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/saws/ontology#Editor -->

    <owl:Class rdf:about="http://purl.org/saws/ontology#Editor">
        <rdfs:subClassOf rdf:resource="http://erlangen-crm.org/current/E21_Person"/>
        <owl:disjointWith rdf:resource="http://purl.org/saws/ontology#Scribe"/>
        <owl:disjointWith rdf:resource="http://purl.org/saws/ontology#Transmitter"/>
        <rdfs:comment xml:lang="en">A modern scholar editing material to produce an edition, in the age of printing and beyond</rdfs:comment>
        <rdfs:label xml:lang="en">editor</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/saws/ontology#EditorSection -->

    <owl:Class rdf:about="http://purl.org/saws/ontology#EditorSection">
        <rdfs:subClassOf rdf:resource="http://purl.org/saws/ontology#Section"/>
        <owl:disjointWith rdf:resource="http://purl.org/saws/ontology#ScribeSection"/>
        <rdfs:comment xml:lang="en">Any division within a linguistic object created by an editor, e.g. to make the navigation of the text clearer or to mark a region of interest</rdfs:comment>
        <rdfs:label xml:lang="en">editor section</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/saws/ontology#EditorTitle -->

